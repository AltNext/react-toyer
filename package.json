{
  "name": "react-toyer",
  "version": "0.1.1",
  "description": "React Toyer",
  "main": "./index.js",
  "types": "./index.d.ts",
  "files": [
    "src/**/*.js",
    "index.js",
    "**/*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/Altnext/react-toyer.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "tsc --build --clean",
    "coverage": "jest --coverage",
    "deduplicate": "yarn-deduplicate -s fewer",
    "docs": "yarn typedoc --disableSources index.ts",
    "lint": "eslint . --cache",
    "prepare": "husky install",
    "prerelease": "yarn build",
    "release": "changeset publish",
    "test": "jest --ci",
    "type": "tsc --noEmit"
  },
  "resolutions": {
    "prettier": "2.4.1"
  },
  "peerDependencies": {
    "react": ">=17"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.4.1",
    "@changesets/cli": "2.27.8",
    "@commitlint/cli": "17.7.0",
    "@commitlint/config-conventional": "13.2.0",
    "@jest/types": "27.2.5",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "12.1.2",
    "@types/jest": "27.0.2",
    "@types/react": "17.0.32",
    "canvas": "2.8.0",
    "cspell": "8.8.2",
    "eslint-config-altnext": "2.2.0",
    "husky": "7.0.4",
    "jest": "29.7.0",
    "jest-junit": "13.0.0",
    "lint-staged": "15.2.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "ts-jest": "29.1.1",
    "ts-node": "10.4.0",
    "typedoc": "0.22.13",
    "typedoc-plugin-markdown": "4.0.0",
    "typescript": "4.4.4",
    "yarn-deduplicate": "3.1.0"
  },
  "author": "Altnext Gorillaz",
  "license": "MIT",
  "eslintIgnore": [
    "dist",
    "node_modules"
  ],
  "lint-staged": {
    "**/*": [
      "cspell lint"
    ],
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --quiet --cache"
    ]
  }
}
